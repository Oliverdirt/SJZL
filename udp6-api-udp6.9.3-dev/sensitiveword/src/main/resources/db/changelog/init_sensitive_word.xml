    <changeSet author="hx" id="1565766470097-34">
        <createTable remarks="敏感词表" tableName="cscp_sensitive_word">
            <column  name="id" remarks="敏感词ID" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="sen_type_id" remarks="敏感词分类ID" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="sen_content" remarks="敏感词内容" type="VARCHAR(100)"/>
            <column name="sen_replace" remarks="敏感词替换" type="VARCHAR(100)"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" remarks="更新时间" type="TIMESTAMP"/>
        </createTable>
    </changeSet>
    <changeSet author="hx" id="1565766470097-35">
        <createTable remarks="敏感词分类表" tableName="cscp_sensitive_word_type">
