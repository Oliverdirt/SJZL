            <column name="sen_type_order" remarks="敏感词分类排序" type="INT"/>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="update_time" remarks="更新时间" type="TIMESTAMP"/>
        </createTable>
    </changeSet>
